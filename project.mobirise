          "_customCSS": "padding-top: (@paddingTop * 15px);\npadding-bottom: (@paddingBottom * 15px);\n& when (@bg-type = 'color') {\n  background-color: @bg-value;\n}\n& when (@bg-type = 'image') {\n  background-image: url(@bg-value);\n}\n.card-icon {\n  font-size: (@iconsSize * 8px);\n  color: @iconsColor;\n  transition: color 0.3s;\n}\n.card-title {\n  font-weight: 600;\n  letter-spacing: 1px;\n}\n.link-ico {\n  font-size: 0.8rem;\n  color: @linkiconsColor;\n  border: 2px solid @linkiconsColor;\n  border-radius: 100%;\n  padding: 0.5rem 0;\n  font-weight: 900;\n  padding-left: 9px;\n  padding-right: 7px;\n}\n.line {\n  width: 35px;\n  height: 3px;\n  display: inline-block;\n  background: @lineColor;\n  margin-bottom: 1.5rem;\n  transition: background 0.3s;\n}\n.card {\n  padding: 0 !important;\n}\n.card-wrapper {\n  &:hover {\n    background: @iconsColor;\n    .card-icon {\n      color: @cardsBg;\n    }\n    .line {\n      background: @cardsBg;\n    }\n  }\n  padding: 4rem;\n  transition: background 0.3s;\n}\n.card1 {\n  background: @cardsBg;\n}\n.card2 {\n  background: lighten(@cardsBg, 2%);\n}\n.card3 {\n  background: lighten(@cardsBg, 4%);\n}\n.card4 {\n  background: lighten(@cardsBg, 6%);\n}\n.card5 {\n  background: lighten(@cardsBg, 8%);\n}\n.card6 {\n  background: lighten(@cardsBg, 10%);\n}\n.col-lg-x {\n  -ms-flex: 0 0 20%;\n  flex: 0 0 20%;\n  max-width: 20%;\n}\n& when (@cardsAmount > '4') {\n  @media (max-width: 1400px) {\n    .col-md-6 {\n      -ms-flex: 0 0 50%;\n      flex: 0 0 50%;\n      max-width: 50%;\n    }\n  }\n}\n@media (max-width: 767px) {\n  .col-12 {\n    -ms-flex: 0 0 100%;\n    flex: 0 0 100%;\n    max-width: 100%;\n  }\n}\n",
